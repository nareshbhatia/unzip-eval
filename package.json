{
  "name": "unzip-eval",
  "description": "Evaluation of unzipping libraries for Node.js",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/unzipper-example.ts",
    "start:debug": "ndb ts-node src/unzipper-example.ts",
    "start:prod": "node dist",
    "dev": "nodemon --watch src -e ts --exec yarn start",
    "build": "tsc",
    "format": "prettier --write README.md 'src/**/*.ts'"
  },
  "dependencies": {
    "adm-zip": "^0.4.16",
    "extract-zip": "^2.0.1",
    "unzipper": "^0.10.11",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/node": "^14.10.3",
    "@types/unzipper": "^0.10.3",
    "@types/yauzl": "^2.9.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
